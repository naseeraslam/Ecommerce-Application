<h4 class="text-black mb-4">Successfully purchased</h4>
<div class="container">
  <h6 class="text-black mb-4">Order History</h6>
  <br>
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>ID</th>
        <th>Customer Name</th>
        <th>Purachse items Id's</th>
        <th>Total Price</th>
        <th>Time</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @order_detail.id %></td>
        <td><%= @order_detail.customer_name %></td>
        <% products = []%>
        <% @order_detail.product_ids.each do |f| %>
          <% products.push(f.to_i)%>
        <%end%>
        <td><%= products %></td>
        <td><%= @order_detail.total_price %></td>
        <td><%= @order_detail.created_at %></td>
      </tr>
    </tbody>
  </table>
